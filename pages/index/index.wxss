.home_container_top{
  width:750rpx;
  height:421rpx;
  background:linear-gradient(180deg,rgba(51,102,255,1),rgba(51,153,255,1));
  border-radius:0px 0px 20rpx 20rpx;
  padding: 20rpx;
  box-sizing: border-box;
}
.home_container_top .home_container_top_name{
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 95%;
  height: 180rpx;
  border-bottom: 1px solid rgba(255,255,255,0.2);
}

.home_container_top_name>view:nth-child(1){
  display: flex;
}

.home_container_top_name>view:nth-child(1)>image{
  width:88rpx;
  height:88rpx;
  border-radius:50%;
  border: 6rpx solid #fff;
}

.home_container_top_name>view:nth-child(1)>view{
  color: #fff;
  margin-left: 15rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  height:88rpx;
}

.home_container_top_name>view:nth-child(1)>view>text:nth-child(1){
  font-size:36rpx;
}

.home_container_top_name>view:nth-child(1)>view>text:nth-child(2){
  font-size:24rpx;
}

.home_container_top_name>view:nth-child(2){
  display: flex;
  justify-content: space-between;
  width: 170rpx;
}

.home_container_top_name>view:nth-child(2)>view{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  color: #fff;
  font-size:24rpx;
}

.home_container_top_name>view:nth-child(2)>view:nth-child(1)>image,.home_container_top_name>view:nth-child(2)>view:nth-child(2)>image{
  width:40rpx;
  height:40rpx;
  margin-bottom: 15rpx;
}

.home_container_top_money{
  height: 240rpx;
  width: 95%;
  color: #fff;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}

.home_container_top_money>view:nth-child(1){
  font-size:32rpx;
  display: flex;
  justify-content: space-between;
}

.home_container_top_money>view:nth-child(2){
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.home_container_top_money>view:nth-child(2)>text{
  font-size: 60rpx;
  font-weight: bold;
}

.home_container_top_money>view:nth-child(2)>view{
  width:160rpx;
  height:60rpx;
  background:rgba(255,255,255,1);
  border-radius:30rpx;
  color: #3399FF;
  text-align: center;
  line-height: 60rpx;
}

