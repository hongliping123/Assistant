  align-items: center;
  
}
.home_container_top_name1{
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 95%;
  height: 180rpx;
  border-bottom: 1px solid #EEEEEE;
  box-sizing: border-box;
}
.home_container_top_name1>view:nth-child(1){
  display: flex;
}

.home_container_top_name1>view:nth-child(1)>image{
  width:88rpx;
  height:88rpx;
  border-radius:50%;
  border: 6rpx solid #EEEEEE;
}

.home_container_top_name1>view:nth-child(1)>view{
  margin-left: 15rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  height:88rpx;
}

.home_container_top_name1>view:nth-child(1)>view>text:nth-child(1){
  font-size:36rpx;
}

.home_container_top_name1>view:nth-child(1)>view>text:nth-child(2){
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(153,153,153,1);
}

.home_container_top_name1>view:nth-child(2){
  display: flex;
  justify-content: center;
  align-items: center;
  width: 250rpx;
}

.home_container_top_name1>view:nth-child(2)>view{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  color: #999999;
  font-size:24rpx;
}

.home_container_top_name1>view:nth-child(2)>view:nth-child(1)>image,.home_container_top_name1>view:nth-child(2)>view:nth-child(2)>image{
  width:40rpx;
  height:40rpx;
  margin-bottom: 15rpx;
}

