.container{
  padding: 0 20rpx;
  box-sizing: border-box;
}

.Order_details_list{
  height: 180rpx;
  border-bottom: 1rpx solid #EEEEEE;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.left{
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-evenly;
}

.left_name>text:nth-child(1){
  color: #3399FF;
  font-size: 28rpx;
}

.left_price{
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(255,153,0,1);
}

.left_time{
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(153,153,153,1);
}

.right_price{
  font-size:36rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(255,153,0,1);
}

.default-center{
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  height: 30vh;
}

.center-img{
  width:360rpx;
  height:300rpx;
}

.default-center>view{
  font-size:30rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(204,204,204,1);
}