.line{
  width:750rpx;
  height:20rpx;
  background:rgba(238,238,238,1);
}

.business{
  padding-left: 21rpx;
}

.business_info{
  height: 100rpx;
  line-height: 100rpx;
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(51,153,255,1);
  border-bottom: 1rpx solid #EEEEEE;
}

.business_info_list{
  height: 88rpx;
  border-bottom: 1rpx solid #EEEEEE;
  display: flex;
  align-items: center;
}

.business_info_list_name{
  width: 70%;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(0,0,0,1);
}

.business_info_list_sl{
  /* width: 20%; */
  display: flex;
}

.business_info_list_sl_img{
  /* width: 200rpx; */
  display: flex;
  align-items: center;
}

.business_info_list_sl_img image{
  width:22rpx;
  height:25rpx;
  margin-right: 10rpx;
}

.business_info_list_sl_img text{
  width: 100rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}

.default-center{
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  height: 30vh;
}

.center-img{
  width:360rpx;
  height:300rpx;
}

.default-center>view{
  font-size:30rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(204,204,204,1);
}

